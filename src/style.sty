\theoremstyle{plain}

\newshadetheorem{thm}{Theorem}[subsection]
%\newshadetheorem*{thm*}{Theorem}
\def\thmautorefname{Theorem}

\newshadetheorem{lem}[thm]{Lemma}
%\newshadetheorem*{lem*}{Lemma}
\def\lemautorefname{Lemma}

\newshadetheorem{conj}[thm]{Conjecture}
%\newshadetheorem*{conj*}{Conjecture}
\def\claimautorefname{Conjecture}

\newshadetheorem{claim}[thm]{Claim}
%\newshadetheorem*{claim*}{Claim}
\def\claimautorefname{Claim}

\newshadetheorem{cor}[thm]{Corollary}
%\newshadetheorem*{cor*}{Corollary}
\def\corautorefname{Corollary}


\theoremstyle{definition}

\newshadetheorem{defn}[thm]{Definition}
%\newshadetheorem*{defn*}{Definition}
\def\defnautorefname{Definition}


\theoremstyle{remark}

\newshadetheorem{remark}[thm]{Remark}
%\newshadetheorem*{remark*}{Remark}
\def\remarkautorefname{Remark}


